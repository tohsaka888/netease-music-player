{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "es6",
    "lazy": true
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2022",
    "loose": true,
    "externalHelpers": true
  },
  "minify": true,
  "sourceMaps": true,
  "isModule": true
}
